'use strict';









jest.unmock('../Platform.ios');
jest.unmock('../Platform.android');




describe('Platform',function(){

describe('OS',function(){
it('should have correct value',function(){
expect(require('../Platform.ios').OS).toEqual('ios');
expect(require('../Platform.android').OS).toEqual('android');});});



describe('select',function(){
it('should return platform specific value',function(){
var obj={ios:'ios',android:'android'};
expect(require('../Platform.ios').select(obj)).toEqual(obj.ios);
expect(require('../Platform.android').select(obj)).toEqual(obj.android);});});});