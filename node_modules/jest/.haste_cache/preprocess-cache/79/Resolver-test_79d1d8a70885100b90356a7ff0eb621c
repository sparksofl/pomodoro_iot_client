var deps=[module];

var depResolver=new (require('../'))({
projectRoot:'/root',
polyfillModuleNames:['some module']});


DependencyGraph.prototype.getDependencies.mockImpl(function(){
return require('promise').resolve(new ResolutionResponseMock({
