'use strict';




jest.unmock('WebSocket');
jest.setMock('NativeModules',{
WebSocketModule:{
connect:function connect(){}}});





describe('WebSocket',function(){

it('should have connection lifecycle constants defined on the class',function(){
expect(require('WebSocket').CONNECTING).toEqual(0);});


it('should have connection lifecycle constants defined on the instance',function(){
expect(new (require('WebSocket'))('wss://echo.websocket.org').CONNECTING).toEqual(0);});});